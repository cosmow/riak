{
    "name": "cosmow/riak",
    "type": "library",
    "description": "Doctrine Riak Abstraction Layer",
    "keywords": ["database", "persistence", "riak"],
    "homepage": "http://www.doctrine-project.org",
    "license": "MIT",
    "authors": [
        { "name": "ckmm" },
    ],
    "require": {
        "php": ">=5.3.2",
        "ext-mongo": "^1.2.12",
        "doctrine/common": "^2.2"
    },
    "require-dev": {
        "jmikola/geojson": "^1.0"
    },
    "suggest": {
        "jmikola/geojson": "Support GeoJSON geometry objects in 2dsphere queries"
    },
    "autoload": {
        "psr-0": { "Doctrine\\Riak": "lib/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.3.x-dev"
        }
    }
}
